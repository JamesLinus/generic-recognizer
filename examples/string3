grammar* = rule { rule } "." ;
rule = #ID [ "*" ] "=" expr ";" ;
expr = term { "|" term } ;
term = factor { factor } ;
factor = #ID
       | "#" #ID
       | #STR2
       | "(" expr ")"
       | "{" expr "}"
       | "[" expr "]"
       | "<" "#" #ID ":" #ID ">"
       | "[[" expr "]]"
       | output
       | control ;
output = "{{" outexpr { outexpr } "}}" ;
outexpr = #STR2 | "*" [ #NUM ] | ";" | "+" | "-" ;
control = "$" ( "push" | "pop" | "eout" | "dout" ) ;

.
